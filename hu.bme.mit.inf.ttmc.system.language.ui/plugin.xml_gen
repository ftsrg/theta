	<!-- quickfix marker resolution generator for hu.bme.mit.inf.ttmc.system.language.SystemLanguage -->
	<extension
	        point="org.eclipse.ui.ide.markerResolution">
	    <markerResolutionGenerator
	        class="hu.bme.mit.inf.ttmc.system.language.ui.SystemLanguageExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
	        markerType="hu.bme.mit.inf.ttmc.system.language.ui.systemlanguage.check.fast">
	        <attribute
	            name="FIXABLE_KEY"
	            value="true">
	        </attribute>
	    </markerResolutionGenerator>
	    <markerResolutionGenerator
	        class="hu.bme.mit.inf.ttmc.system.language.ui.SystemLanguageExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
	        markerType="hu.bme.mit.inf.ttmc.system.language.ui.systemlanguage.check.normal">
	        <attribute
	            name="FIXABLE_KEY"
	            value="true">
	        </attribute>
	    </markerResolutionGenerator>
	    <markerResolutionGenerator
	        class="hu.bme.mit.inf.ttmc.system.language.ui.SystemLanguageExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
	        markerType="hu.bme.mit.inf.ttmc.system.language.ui.systemlanguage.check.expensive">
	        <attribute
	            name="FIXABLE_KEY"
	            value="true">
	        </attribute>
	    </markerResolutionGenerator>
	</extension>
